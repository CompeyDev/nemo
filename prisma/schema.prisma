datasource db {
    provider = "sqlite"
    url      = "file:dev.db"
}

generator client {
    provider = "go run github.com/prisma/prisma-client-go"
    // set the output folder and package name
    // output           = "./your-folder"
    // package          = "yourpackagename"
}

model PayloadClient {
    id          String   @id
    name        String
    lastCheckIn DateTime @updatedAt
}
